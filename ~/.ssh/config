# Replit SSH Configuration
Host replit
    HostName ssh.replit.com
    User git
    IdentityFile ~/.ssh/replit
    IdentitiesOnly yes

# Replit Development Environment
Host *.replit.dev
    User git
    IdentityFile ~/.ssh/replit
    IdentitiesOnly yes
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ServerAliveInterval 60
    ServerAliveCountMax 3
